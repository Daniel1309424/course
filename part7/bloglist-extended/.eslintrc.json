{
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "plugins": ["react", "prettier"],
    "env": {
      "browser": true,
      "node": true,
      "es2020": true
    },
    "rules": {
      "prettier/prettier": "error",
      "react/prop-types": "off"
    }
  }
  